{
	"name": "@fluid-internal/devtools-browser-extension",
	"version": "2.0.0-internal.7.4.0",
	"private": true,
	"description": "A browser extension for visualizing Fluid Framework stats and operations",
	"homepage": "https://fluidframework.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/FluidFramework.git",
		"directory": "packages/tools/devtools/devtools-browser-extension"
	},
	"license": "MIT",
	"author": "Microsoft and contributors",
	"scripts": {
		"build": "fluid-build . --task build",
		"build-and-test": "npm run build && npm run test",
		"build:compile": "fluid-build . --task compile",
		"build:test": "tsc --project ./e2e-tests/tsconfig.json",
		"build:webpack": "npm run webpack",
		"clean": "rimraf --glob coverage dist nyc \"**/*.tsbuildinfo\" \"**/*.build.log\"",
		"eslint": "eslint src",
		"eslint:fix": "eslint src --fix",
		"format": "npm run prettier:fix",
		"format-and-build": "npm run format && npm run build",
		"format-and-compile": "npm run format && npm run build:compile",
		"good-fences": "gf",
		"lint": "npm run prettier && npm run good-fences && npm run eslint",
		"lint:fix": "npm run prettier:fix && npm run eslint:fix",
		"mocha": "mocha \"dist/tsc/test/**/*.test.js\" --exit",
		"prettier": "prettier --check . --cache --ignore-path ../../../../.prettierignore",
		"prettier:fix": "prettier --write . --cache --ignore-path ../../../../.prettierignore",
		"rebuild": "npm run clean && npm run build",
		"start:client:test": "webpack serve --config webpack.test.js",
		"test": "concurrently npm:test:mocha npm:test:jest",
		"test:coverage": "c8 npm test",
		"test:end-to-end": "npm run test:jest",
		"test:jest": "jest",
		"test:jest:verbose": "cross-env FLUID_TEST_VERBOSE=1 jest",
		"test:mocha": "npm run mocha",
		"test:mocha:verbose": "cross-env FLUID_TEST_VERBOSE=1 npm run mocha -- --verbose",
		"test:unit": "npm run test:mocha",
		"test:verbose": "concurrently npm:test:mocha:verbose npm:test:jest:verbose",
		"tsc": "tsc",
		"webpack": "webpack",
		"webpack:dev": "webpack --env development"
	},
	"c8": {
		"all": true,
		"cache-dir": "nyc/.cache",
		"exclude": [
			"src/test/**/*.ts",
			"dist/tsc/test/**/*.js",
			"dist/e2e-tests/**",
			"dist/bundle/**"
		],
		"exclude-after-remap": false,
		"include": [
			"src/**/*.ts",
			"dist/tsc/**/*.js"
		],
		"report-dir": "nyc/report",
		"reporter": [
			"cobertura",
			"html",
			"text"
		],
		"temp-directory": "nyc/.nyc_output"
	},
	"dependencies": {
		"@fluentui/react": "^8.112.9",
		"@fluid-experimental/devtools-core": "workspace:~",
		"@fluid-experimental/devtools-view": "workspace:~",
		"@fluid-internal/client-utils": "workspace:~",
		"@fluidframework/core-interfaces": "workspace:~",
		"@fluidframework/core-utils": "workspace:~",
		"@microsoft/1ds-core-js": "^3.2.15",
		"@microsoft/1ds-post-js": "^3.2.15",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"tslib": "^2.6.2"
	},
	"devDependencies": {
		"@fluid-example/example-utils": "workspace:~",
		"@fluid-experimental/react-inputs": "workspace:~",
		"@fluidframework/aqueduct": "workspace:~",
		"@fluidframework/build-common": "^2.0.3",
		"@fluidframework/build-tools": "^0.28.0",
		"@fluidframework/container-definitions": "workspace:~",
		"@fluidframework/container-loader": "workspace:~",
		"@fluidframework/container-runtime-definitions": "workspace:~",
		"@fluidframework/eslint-config-fluid": "^3.1.0",
		"@fluidframework/mocha-test-setup": "workspace:~",
		"@fluidframework/runtime-utils": "workspace:~",
		"@fluidframework/sequence": "workspace:~",
		"@fluidframework/test-utils": "workspace:~",
		"@microsoft/api-extractor": "^7.38.3",
		"@types/chai": "^4.3.11",
		"@types/chrome": "0.0.253",
		"@types/expect-puppeteer": "5.0.6",
		"@types/jest": "29.5.10",
		"@types/jest-environment-puppeteer": "5.0.6",
		"@types/jsdom": "^21.1.6",
		"@types/jsdom-global": "^3.0.7",
		"@types/mocha": "^10.0.6",
		"@types/node": "^18.17.1",
		"@types/proxyquire": "^1.3.31",
		"@types/puppeteer": "7.0.4",
		"@types/react": "^17.0.44",
		"@types/react-dom": "^17.0.18",
		"@types/sinon": "^17.0.2",
		"@types/sinon-chrome": "^2.2.15",
		"c8": "^8.0.1",
		"chai": "^4.3.10",
		"concurrently": "^8.2.2",
		"copy-webpack-plugin": "^11.0.0",
		"cross-env": "^7.0.3",
		"dotenv-webpack": "^8.0.1",
		"eslint": "~8.50.0",
		"eslint-config-prettier": "~9.0.0",
		"eslint-plugin-chai-expect": "~3.0.0",
		"good-fences": "^1.2.0",
		"html-webpack-plugin": "^5.5.3",
		"jest": "^29.7.0",
		"jest-dev-server": "^9.0.1",
		"jest-environment-puppeteer": "^9.0.1",
		"jest-junit": "^16.0.0",
		"jest-puppeteer": "^9.0.1",
		"jsdom": "^23.0.0",
		"jsdom-global": "^3.0.2",
		"mocha": "^10.2.0",
		"prettier": "~3.0.3",
		"proxyquire": "^2.1.3",
		"puppeteer": "^21.5.2",
		"rimraf": "^5.0.5",
		"sinon": "^17.0.1",
		"sinon-chrome": "^3.0.1",
		"start-server-and-test": "^2.0.3",
		"ts-jest": "^29.1.1",
		"ts-loader": "^9.5.1",
		"typescript": "~5.1.6",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4"
	},
	"fluidBuild": {
		"tasks": {
			"compile": {
				"dependsOn": [
					"...",
					"webpack"
				],
				"script": false
			}
		}
	},
	"typeValidation": {
		"disabled": true,
		"broken": {}
	}
}
