{
	"name": "@fluid-experimental/property-query",
	"version": "2.0.0-internal.7.4.0",
	"description": "QueryService implementation",
	"homepage": "https://fluidframework.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/FluidFramework.git",
