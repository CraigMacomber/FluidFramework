		"mocha-json-output-reporter": "^2.1.0",
		"mocha-junit-reporter": "^2.2.1",
		"mocha-multi-reporters": "^1.5.1",
		"mocha-xunit-reporter": "2.3.0",
		"mochawesome": "7.1.3",
		"moment": "^2.29.4",
		"npm-run-all": "4.1.5",
