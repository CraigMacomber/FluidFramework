{
	"name": "@fluid-experimental/property-query-service",
	"version": "2.0.0-internal.7.4.0",
	"private": true,
	"description": "Moira service",
	"homepage": "https://fluidframework.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/FluidFramework.git",
		"directory": "experimental/PropertyDDS/services/property-query-service"
	},
	"license": "MIT",
	"author": "Microsoft and contributors",
	"main": "server.js",
	"files": [
		"config",
		"src",
		"server.js"
	],
	"scripts": {
		"clean": "rimraf --glob nyc",
		"coverage:all": "c8 mocha",
		"coverage:basic": "RANDOMIZE_FILE_ORDER=1 nyc --report-dir coverage/basic mocha --opts test/basic/mocha.opts",
		"coverage:db": "c8 --report-dir coverage/db mocha --opts test/db/mocha.opts",
		"coverage:endtoend": "c8 --report-dir coverage/endtoend mocha --opts test/endtoend/mocha.opts",
		"coverage": "run-s -c init:default coverage:all term",
		"format": "npm run prettier:fix",
		"lint:fix": "npm run prettier:fix",
		"lint": "npm run prettier",
		"mocha:basic": "mocha --opts test/basic/mocha.opts",
		"mocha:db": "mocha --opts test/db/mocha.opts",
		"mocha:endtoend": "mocha --opts test/endtoend/mocha.opts",
		"mocha": "mocha",
		"prettier:fix": "prettier --write . --cache --ignore-path ../../../../.prettierignore",
		"prettier": "prettier --check . --cache --ignore-path ../../../../.prettierignore",
		"start": "node server.js",
		"test:coverage": "c8 npm test",
		"test:mocha:verbose": "cross-env FLUID_TEST_VERBOSE=1 npm run test:mocha",
		"test:mocha": "mocha --exit -r node_modules/@fluidframework/mocha-test-setup",
		"test": "npm run test:mocha"
	},
	"c8": {
		"report-dir": "nyc/report",
		"reporter": [
			"lcov",
			"json",
			"text"
		],
		"sourceMap": false,
		"tempDirectory": "coverage/tmp"
	},
